#!/usr/bin/ruby

# a(1) = 2, a(n+1) = a(n)-th squarefree number.
# https://oeis.org/A071255

# a(n) = A005117(a(n-1)+1), a(1) = 2.
# Limit_{n->infinity} a(n+1)/a(n) = zeta(2). - ~~~~

# Cf. A013661.

var n = 1
var prev = n+1

for (1..100) {
    n = nth_squarefree(n+1)
    assert_eq(n.squarefree_count, prev)
    print(n, ", ")
    prev = n+1
}

__END__

