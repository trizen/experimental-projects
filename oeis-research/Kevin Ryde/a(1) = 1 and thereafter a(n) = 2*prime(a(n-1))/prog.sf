#!/usr/bin/ruby

# a(1) = 1 and thereafter a(n) = 2*prime(a(n-1)).
# https://oeis.org/A352289

# a(10)-a(15) from ~~~~

func prime(n) is cached {
    n.prime
    #Num(`./primecount -n #{n}`.strip)
}

func a(n) is cached {
    return 1 if (n == 1)
    2*prime(a(n-1))
}

for n in (1..100) {
    print(a(n), ", ")
}

