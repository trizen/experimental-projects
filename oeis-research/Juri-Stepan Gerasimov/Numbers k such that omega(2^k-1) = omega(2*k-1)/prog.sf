#!/usr/bin/ruby

# Numbers k such that omega(2^k-1) = omega(2*k-1), where omega(n) = A001221(n).
# https://oeis.org/A355805

# Known terms:
#   1, 2, 3, 5, 7, 11, 13, 19, 23, 31, 53, 59, 61, 67, 101, 103, 109, 131, 149, 167, 179, 193, 197, 227, 241, 269, 353, 373, 389, 443, 457, 487, 599, 607, 881, 941, 971, 1063, 1103, 1117, 1193

include("../../../factordb/auto.sf")

for k in (1..1e4) {
    if (omega(2**k - 1) == omega(2*k - 1)) {
        print(k, ", ")
    }
}

__END__

