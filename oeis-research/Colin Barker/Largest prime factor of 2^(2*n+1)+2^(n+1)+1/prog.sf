#!/usr/bin/ruby

# Largest prime factor of 2^(2*n+1)+2^(n+1)+1.
# https://oeis.org/A229768

include('../../../factordb/auto.sf')

func a(n) {
     2**(2*n + 1) + 2**(n+1) + 1
}

var fh = File('bfile.txt').open_w

fh.autoflush(true)

for n in (1..10000) {
    var row = "#{n} #{gpf(a(n))}"
    say row
    fh.say(row)
}
