#!/usr/bin/ruby

# a(n) = largest prime factor of the number with decimal expansion 20305070...0p_n where p_n = n-th prime.
# https://oeis.org/A308899

DATA.lines.each_kv {|n,line|

    say "Testing: #{n+1}"

    var p = line.nums[-1]
    var k = primes(prime(n+1)).join('0').to_i

    assert(k % p == 0)
    assert(p.is_prime)

    assert((k / p).gpf < p)
}

