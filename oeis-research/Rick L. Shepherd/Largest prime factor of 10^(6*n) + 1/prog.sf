#!/usr/bin/ruby

# Largest prime factor of 10^(6*n) + 1.
# https://oeis.org/A072848

include('../../../factordb/auto.sf')

func a(n) is cached {
    10**(6*n) + 1
}

var fh = File('bfile.txt').open_w

fh.autoflush(true)

for n in (1..10000) {
    var row = "#{n} #{gpf(a(n))}"
    say row
    fh.say(row)
}

__END__

