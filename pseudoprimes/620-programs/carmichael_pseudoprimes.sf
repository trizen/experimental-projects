#!/usr/bin/ruby

var carmichael_pseudoprimes = []

glob("../*.txt").map{File(_).open_r.lines...}.each {|line|
    line ~~ /\S/ || next

    if (line =~ /: (\d+)/) {|m|

        var n = Num(m[0])

        if (n.is_carmichael) {
            carmichael_pseudoprimes << n
        }
    }
}

carmichael_pseudoprimes.uniq.sort.each {|n|
    say n
}

__END__
