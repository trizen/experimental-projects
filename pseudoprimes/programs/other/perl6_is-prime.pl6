#!/usr/bin/perl6

for @*ARGS -> $file {
    for $file.IO.lines -> $line {

        next if $line ~~ /^\h*'#'/;
        $line ~~ /\S/ or next;
        my $n = (split(' ', $line))[*-1];
        $n ~~ /^<[0..9]>+$/ or next;

        if (is-prime($n)) {
            say "Counter-example: $n";
        }
    }
}

